{
  "testURL": "http://localhost:8888/",  
  "maxConcurrency": 1,
  "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "vue"
    ],
    "rootDir": "..",
    "testEnvironment": "node",
    "testRegex": "/e2e/.*\\.e2e\\.spec\\.(ts|js)$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist",
      "<rootDir>/src/dist",
      "<rootDir>/.git"
    ]
}
